### DOCKER COMMANDS ###
alias startDocker="~/Documents/Docker/startDocker.sh"
alias stopDocker="~/Documents/Docker/stopDocker.sh"

### MINIKUBE COMMANDS ###
alias miniStart='minikube start --addons=dashboard --addons=metrics-server --addons="ingress" --addons="ingress-dns" '
alias miniStop="minikube stop"

### GENERIC ALIASES ###
alias k="kubectl"

########### TERRAFORM ##############
